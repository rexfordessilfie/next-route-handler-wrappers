{
  "name": "@nextwrappers/async-local-storage",
  "version": "1.1.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./pagesapi": {
      "import": "./dist/pagesapi/index.js",
      "require": "./dist/pagesapi/index.cjs",
      "types": "./dist/pagesapi/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "pagesapi": [
        "dist/pagesapi/index.d.ts"
      ]
    }
  },
  "license": "MIT",
  "author": "Rexford Essilfie <rexfordessilfie09@gmail.com>",
  "repository": "https://github.com/rexfordessilfie/nextwrappers",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "yarn build --watch",
    "test": "ava",
    "prepublishOnly": "yarn clean && yarn build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "next",
    "route",
    "handler",
    "wrapper",
    "middleware",
    "api"
  ],
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--import=tsimp"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "ava": "^5.3.0",
    "next": "^13.4.7",
    "node-mocks-http": "^1.12.2",
    "tsimp": "^2.0.12",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@nextwrappers/core": "workspace:*",
    "path-to-regexp": "^6.2.1"
  },
  "peerDependencies": {
    "next": "^12"
  }
}
