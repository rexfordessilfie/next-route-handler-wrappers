{
  "compilerOptions": {
    "target": "es2020",
    "module": "Node16",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "noEmit": true,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node16"
  }
}
